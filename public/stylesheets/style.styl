@import 'nib';

$controlBackgroundColor = rgb(44,44,44)
$controlForegroundColor = rgb(255,255,255)
$controlGradientColor = rgb(34,34,34)
$controlGradientHighlightColor = rgb(51,51,51)
$controlEdgeHightlightColor = rgb(61,61,61)

body
  background-image: url('/images/light_toast.png')
  font-family: 'Ubuntu Mono', monospace
  margin: 0px
  padding: 0px

a
  color: #00B7FF

fieldset
  border: 0px
  padding: 0px
  margin: 0px


.main-header
  line-height: 24px
  height: 24px
  color: rgb(246,242,239)
  padding: 10px
  margin: 0px 0px 20px 0px
  box-shadow: 0 3px 5px rgba(0,0,0, .20)
  background: linear-gradient(top, rgb(76,76,76), rgb(63,63,63))


.main-content
  margin-left: 60px
  margin-right: 60px

// ui-rating

.ui-rating
  width: 175px
  height: 26px
  background-repeat: no-repeat
  background-image: url('data:image/png;base64,')
  padding: 4px 0px 0px 73px

.ui-rating
  .ui-rating-star-container
    width: 95px
    height: 18px
    display: inline-block

.ui-rating
  .ui-rating-star-container
    .ui-rating-star
      width: 18px
      height: 18px
      padding: 0px 1px 0px 0px
      background-repeat: no-repeat
      background-image: url('data:image/png;base64,')
      display: inline-block
      cursor: pointer

.ui-rating
  .ui-rating-star-container
    .ui-rating-star.full
      background-image: url('data:image/png;base64,')

.ui-rating
  .ui-rating-star-container
    .ui-rating-star.empty
      background-image: none

// heavy header

.heavy-well
  background-color: rgb(235,235,239)
  border-radius: 5px 5px 0px 0px
  box-shadow: 0 2px 18px rgb(10,10,10)

.heavy-header
  background-color: rgb(70,70,70)
  height: 45px
  border-radius: 5px 5px 0px 0px
  color: rgb(215, 213, 205)
  margin-bottom: 15px

.light-well
  border-radius: 5px 5px 5px 5px
  background-color: rgb(235,235,239)
  border: 1px solid rgb(204,205,199)
  margin-bottom: 20px
  opacity: 50%
  box-shadow: inset 0 3px 30px rgba(0, 0, 0, .15)

.light-well-header
  background-color: rgb(246,242,239)
  height: 32px
  line-height: 32px
  padding: 0px 10px 0px 10px
  border-radius: 5px 5px 0px 0px
  border-bottom: 1px solid rgb(208,208,203)
  box-shadow: 0 3px 5px rgba(0,0,0, .10)

.light-well-inner
  padding: 10px
  border-top: 1px solid rgb(184,184,179)


// ui-message-list

.ui-message-list
  background-color: rgb(88,88,88)
  background-repeat: repeat-x
  height: 224px
  overflow-y: auto
  color: rgb(234,228,221)

.ui-message-list ul
  list-style-type: none
  padding: 4px
  margin: 0px

.ui-message-list li
  word-wrap: break-word
  font-size: 14px
  line-height: 18px
  height: 18px
  opacity: 0

.ui-message-list li:nth-child(even)
  background-color: rgb(93,93,93)

.ui-message-list-source:before
  content: "<"

.ui-message-list-source:after
  content: "> "

.ui-message-list-timestamp
  color: rgb(150,150,150)

.ui-message-list-timestamp:after
  content: " "


// chatroom-message-list

.chatroom-message-list-alert
  color: rgb(194,93,93)

.chatroom-message-list-alert .ui-message-list-source
  display: none

.chatroom-message-list-alert .ui-message-list-text
  font-style: italic

.chatroom-message-list-directed .ui-message-list-source
  color: yellow

.chatroom-input-box-text
  width: 100%
  line-height: 24px
  height: 28px
  box-sizing: border-box
  margin: 4px 0px 4px 0px
  padding: 0px 6px 0px 6px
  border: 1px solid rgb(204,205,199)
  border-radius: 0px 0px 5px 5px
  background-color: rgb(246,242,239)
