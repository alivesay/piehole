@import 'nib';

$controlBackgroundColor = rgb(44,44,44)
$controlForegroundColor = rgb(255,255,255)
$controlGradientColor = rgb(34,34,34)
$controlGradientHighlightColor = rgb(51,51,51)
$controlEdgeHightlightColor = rgb(61,61,61)

body
  background-image: url('/images/bg.png')
  background-image: url('data:image/png;base64,')
  background-repeat: repeat
  font-family: 'Ubuntu Mono', monospace

a
  color: #00B7FF

.navbar
  .navbar-inner
    background: linear-gradient(top, $controlGradientHighlightColor, $controlGradientColor)

.main-content
  margin-left: 60px
  margin-right: 60px

// ui-rating

.ui-rating
  width: 175px
  height: 26px
  background-repeat: no-repeat
  background-image: url('data:image/png;base64,')
  padding: 4px 0px 0px 73px

.ui-rating
  .ui-rating-star-container
    width: 95px
    height: 18px
    display: inline-block

.ui-rating
  .ui-rating-star-container
    .ui-rating-star
      width: 18px
      height: 18px
      padding: 0px 1px 0px 0px
      background-repeat: no-repeat
      background-image: url('data:image/png;base64,')
      display: inline-block
      cursor: pointer

.ui-rating
  .ui-rating-star-container
    .ui-rating-star.full
      background-image: url('data:image/png;base64,')

.ui-rating
  .ui-rating-star-container
    .ui-rating-star.empty
      background-image: none


// heavy header

.heavy-well
  background-color: rgb(235,235,239)
  border-radius: 5px 5px 0px 0px
  box-shadow: 0 2px 18px rgb(10,10,10)

.heavy-header
  background-color: rgb(70,70,70)
  height: 45px
  border-radius: 5px 5px 0px 0px
  color: rgb(215, 213, 205)
  margin-bottom: 15px


// ui-chatroom

.ui-message-list
  background-color: rgb(222, 222, 216)
  overflow: auto
  height: 200px
  margin: 20px

.ui-message-list ul
  list-style-type: none
  margin: 10px
  padding: 0px

.ui-message-list li
  background-color: rgb(240,240,236)
  border-top: 1px solid rgb(243,243,243)
  border-right: 1px solid rgb(206,206,206)
  border-bottom: 1px solid rgb(206,206,206)
  border-left: 1px solid rgb(243,243,243)
  padding: 4px 0px 4px 0px
  opacity: 0
  word-wrap: break-word

.ui-message-list-source:before
  content: "<"

.ui-message-list-source:after
  content: ">"


@css {
  [class^="icon-"], [class*=" icon-"]
      background:none!important;
}
